  "description": "Brownfield Development",
  "main": ".eslintrc.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "lint": "eslint . --ext .js,.html",
    "lint:fix": "eslint . --ext .js,.html --fix",
    "format": "prettier --write \"**/*.{html,css,js}\"",
    "format:check": "prettier --check \"**/*.{html,css,js}\"",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "prepare": "husky install",
    "jsdoc": "jsdoc src/**/*.js --destination docs"
  },
