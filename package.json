  "author": "",
  "license": "ISC",
  "devDependencies": {
    "eslint": "^8.54.0",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "jsdoc": "^4.0.4",
    "prettier": "^3.1.0"
  }
}
