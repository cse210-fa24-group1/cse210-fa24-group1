{
    "source": {
      "include": ["src", "__tests__"],
      "includePattern": ".+\\.(js|html)$",
      "excludePattern": "(node_modules/|docs)"
    },
    "plugins": [
      "plugins/markdown"
    ],
    "opts": {
      "destination": "./docs/",
      "recurse": true,
      "readme": "./README.md"
    },
    "templates": {
      "cleverLinks": true,
      "monospaceLinks": true
    },
    "tags": {
      "allowUnknownTags": true
    }
  }